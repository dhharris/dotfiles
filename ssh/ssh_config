Host github.com
  IdentityFile ~/.ssh/id_ed25519_github
  IdentitiesOnly yes

Host gitlab.com
  IdentityFile ~/.ssh/id_ed25519_gitlab
  IdentitiesOnly yes

Host *
 IgnoreUnknown AddKeysToAgent,UseKeychain
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa
 ControlMaster auto
 ControlPath ~/.ssh/controlmasters/%r@%h:%p
 ControlPersist 6h
